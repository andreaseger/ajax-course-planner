<article id="schedule">
  <header>
    <h2 id="schedule-header">Schedule:</h2>
    <a id="schedule-permalink" href="{{permalink}}" data-booking-keys="{{keys}}">Permalink</a>
  </header>
  {{#bookings.days}}
    <section id="day-{{label}}" class="day">
      <header><h4>{{name}}</h4></header>
      {{#times}}
        <section id="time-{{label}}" class="time">
          {{#bookings}}
            {{>booking}}
          {{/bookings}}
          &nbsp;
        </section>
      {{/times}}
    </section>
  {{/bookings.days}}
  <footer>
    <a id="clear_cookie" href='#'>clear schedule</a>
  </footer>
</article>
